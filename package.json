{
  "name": "side-quest-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --host 0.0.0.0 --disable-host-check",
    "build": "ng build --aot --prod",
    "test": "exit",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "yarn run build && deploy-to-gh-pages docs",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run SideQuestWebsite:server:production --bundleDependencies none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx|ts|tsx)\"",
      "post-commit": "git update-index -g"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@types/highlightjs": "^9.12.0",
    "@types/masonry-layout": "^4.2.1",
    "@types/uuid": "^3.4.5",
    "angular-ng-autocomplete": "^1.1.17",
    "bufferutil": "^4.0.1",
    "chart.js": "^2.8.0",
    "cross-fetch": "^3.0.4",
    "express": "^4.15.2",
    "highlight.js": "^9.16.2",
    "husky": "^3.0.0",
    "isomorphic-ws": "^4.0.1",
    "jquery": "^3.4.1",
    "js-video-url-parser": "^0.4.0",
    "masonry-layout": "^4.2.2",
    "ng-lazyload-image": "^7.0.1",
    "ng-recaptcha": "^5.0.0",
    "ng-simple-slideshow": "^1.2.9",
    "ng2-adsense": "^6.0.2",
    "ng2-charts": "^2.3.0",
    "ngx-daterangepicker-material": "^2.1.7",
    "ngx-highlightjs": "^3.0.3",
    "ngx-lightbox": "2.1.2",
    "ngx-masonry": "^1.1.2",
    "ngx-materialize": "^6.1.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "utf-8-validate": "^5.0.2",
    "uuid": "^3.3.2",
    "webpack-node-externals": "^1.7.2",
    "ws": "^7.2.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "deploy-to-gh-pages": "^1.3.7",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-jquery": "^0.2.4",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
