    "@angular/router": "~8.0.1",
    "@types/highlightjs": "^9.12.0",
    "@types/masonry-layout": "^4.2.1",
    "@types/uuid": "^3.4.5",
    "chart.js": "^2.8.0",
    "highlight.js": "^9.16.2",
    "husky": "^3.0.0",
    "js-video-url-parser": "^0.4.0",
